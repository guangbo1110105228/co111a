/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a ,out=Nota);
    Not(in=b ,out=Notb);
    And(a=Nota ,b=b ,out=Anda2b);
    And(a=a ,b=Notb ,out=Andab2);
    Or(a=Anda2b ,b=Andab2 ,out=out);
}